<script>
    document.addEventListener('DOMContentLoaded', () => {
        // Get all elements with the 'data-route' attribute
        const allNavLinks = document.querySelectorAll('[data-route]');

        allNavLinks.forEach(link => {
            link.style.cursor = 'pointer'; // Optional: show it's clickable

            link.addEventListener('click', (event) => {
                // Prevent any default browser action (though none are set here)
                event.preventDefault(); 
                
                // Get the target route from the data-route attribute
                const targetRoute = link.getAttribute('data-route');

                // Navigate using JavaScript
                window.location.href = targetRoute;
            });
        });
    });
</script>

